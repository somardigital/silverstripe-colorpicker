{
    "name": "silverstripe/colorpicker",
    "description": "Color picker fields built in react",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0 || ^5.0",
        "silverstripe/admin": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Colorpicker\\": "src/",
            "SilverStripe\\Colorpicker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ],
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
